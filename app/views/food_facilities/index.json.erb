[
  <% @food_facilities.each do |food_facility| %>
  {
    <%= 'applicant           : #{food_facility["applicant"]},' %>
    <%= 'address             : #{food_facility["address"]},' %>
    <%= 'locationDescription : #{food_facility["locationdescription"]},' %>
    <%= 'foodItems           : #{food_facility["fooditems"]},' %>
    <%= 'daysHours           : #{food_facility["dayshours"]},' %>
    <%= 'facilityType        : #{food_facility["facilitytype"]},' %>
    <%= 'longitude           : #{food_facility["location"]["longitude"]},' %>
    <%= 'latitude           : #{food_facility["location"]["latitude"]}' %>
  }<%= "," unless food_facility = @food_facilities.last %>
  <% end %>
]